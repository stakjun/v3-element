{
  "name": "v3-element",
  "version": "1.10.1",
  "description": "Vue3 component library",
  "type": "module",
  "author": "Stake Luo",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "Vue3"
  ],
  "homepage": "https://stakjun.github.io/v3-element/",
  "repository": {
    "type": "git",
    "url": "https://github.com/stakjun/vue3-element"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/umd/vue3-element.umd.cjs",
  "module": "./dist/es/vue3-element.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/vue3-element.js",
      "require": "./dist/umd/vue3-element.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build-only && npm run move-style",
    "build-only": "run-p build-umd build-es",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.6",
    "async-validator": "^4.2.5"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.17.0",
    "lodash-es": "^4.17.21",
    "move-file-cli": "^3.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "1.0.0-alpha.65",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  }
}
